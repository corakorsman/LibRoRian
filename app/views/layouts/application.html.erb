<!DOCTYPE html>
<html>
<head>
  <title>LibRoRian</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>

<body>

<% flash.each do |key, value| %>
  <div class='flash' id='<%= key %>'>
    <%= value %>
  </div>
  <% end %>

  <%= yield %>

</body>
</html>
